{
  "manifest_version": 3,
  "name": "YouTube Watched Percentage",
  "version": "1.0",
  "description": "Shows the percentage of a YouTube video that has been watched.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
